# Example environment file for YouTube MCP Server
# Copy this to .env and fill in your actual values

# Puch authentication
AUTH_TOKEN_2=your_auth_token_here
MY_NUMBER=your_phone_number_here

# Google API key for YouTube video length (optional but recommended)
GOOGLE_API_KEY=your_google_api_key_here

# Gemini API keys for parallel processing
# At least GEMINI_API_KEY is required
GEMINI_API_KEY=your_primary_gemini_api_key_here

# Optional: Additional Gemini API keys for parallel processing (recommended for videos < 5000 seconds)
GEMINI_API_KEY_2=your_secondary_gemini_api_key_here
GEMINI_API_KEY_3=your_tertiary_gemini_api_key_here
